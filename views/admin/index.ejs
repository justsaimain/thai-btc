<%- include('./partials/head.ejs') %> 

<style>
  .nav-list{
    list-style: none;
    padding: 0;
  }

  .nav-item{
    margin-bottom: 10px;
    background-color: #3a86ff;
    padding: 20px;
    border-radius: 5px;
    font-size: 18px;
  }

  .nav-item a{
    color: #fff;
    text-decoration: none;
  }

  .switch {
    position: relative;
    display: inline-block;
    width: 40px;
    height: 25px;
  }

  .switch input {
    opacity: 0;
    width: 0;
    height: 0;
  }

  .slider {
    position: absolute;
    cursor: pointer;
    top: 0;
    left: -10px;
    right: 0;
    bottom: 0;
    right: 20px;
    width: 50px;
    background-color: #ccc;
    -webkit-transition: .4s;
    transition: .4s;
  }

  .slider:before {
    position: absolute;
    content: "";
    height: 20px;
    width: 20px;
    right: 28px;
    left: 2px;
    top: 2.5px;
    background-color: white;
    box-shadow: 4px 3px 10px 0px rgba(0, 0, 0, 0.047);
    -webkit-transition: .4s;
    transition: .4s;
  }

  input:checked + .slider {
    background-color: #3a86ff;
  }

  input:focus + .slider {
    box-shadow: 0 0 1px #3a86ff;
  }

  input:checked + .slider:before {
    -webkit-transform: translateX(26px);
    -ms-transform: translateX(26px);
    transform: translateX(26px);
  }

  .slider.round {
    border-radius: 25px;
  }

  .slider.round:before {
    border-radius: 50%;
  }

  .twod-status-label{
    color: #3a86ff;
    font-size: 18px;
  }

</style>

  <body>
