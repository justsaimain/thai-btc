    <div class="mod-div">
      <div class="morning-div">
        <div>
          <p class="mod-title">Modern</p>
          <p class="mod-result" id="morning-mod"><%= today.modNet[0] ? today.modNet[0].mod : "--" %></p>
        </div>
        <div>
          <p class="mod-time">9:30 AM</p>
        </div>
        <div>
          <p class="mod-title">Internet</p>
          <p class="mod-result" id="morning-net"><%= today.modNet[0] ? today.modNet[0].net : "--" %></p>
        </div>
      </div>
      <div class="noon-div">
        <div>
          <p class="mod-title">Modern</p>
          <p class="mod-result" id="noon-mod"><%= today.modNet[0] ? today.modNet[0].mod : "--" %></p>
        </div>
        <div>
          <p class="mod-time">2:00 PM</p>
        </div>
        <div>
          <p class="mod-title">Internet</p>
          <p class="mod-result" id="noon-net"><%= today.modNet[0] ? today.modNet[1].net : "--" %></p>
        </div>
      </div>
    </div>
